// This class computes the gcd of m and n by using the Euclidean
// algorithm.
//
// gcd() method written by Chris Kosior

public class Euclid extends GCDAlgorithm
{
	public Euclid()
	{
		super("Euclidean Algorithm");
	}


	public int gcd(int m, int n)
	{
		if(n == 0) {
			return m;
		}
		else gcd(n, m % n);
	}
}
